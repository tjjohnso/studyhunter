<%= form_for @assignment do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :project_class_id %><br />
    <%= f.text_field :project_class_id %>
  </p>
  <p>
    <%= f.label :university_id %><br />
    <%= f.text_field :university_id %>
  </p>
  <p>
    <%= f.label :course_id %><br />
    <%= f.text_field :course_id %>
  </p>
  <p><%= f.submit %></p>
<% end %>
