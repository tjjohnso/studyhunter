<h1>Listing project_memberships</h1>

<table>
  <tr>
    <th>Project instance</th>
    <th>Student</th>
    <th>Student rights</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @project_memberships.each do |project_membership| %>
  <tr>
    <td><%= project_membership.project_instance_id %></td>
    <td><%= project_membership.student_id %></td>
    <td><%= project_membership.student_rights %></td>
    <td><%= link_to 'Show', project_membership %></td>
    <td><%= link_to 'Edit', edit_project_membership_path(project_membership) %></td>
    <td><%= link_to 'Remove', project_membership, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

