<h1>Listing study_sessions</h1>

<table>
  <tr>
    <th>Project instance</th>
    <th>Location</th>
    <th>Meeting date</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @study_sessions.each do |study_session| %>
  <tr>
    <td><%= study_session.project_instance_id %></td>
    <td><%= study_session.location_id %></td>
    <td><%= study_session.meeting_date %></td>
    <td><%= link_to 'Show', study_session %></td>
    <td><%= link_to 'Edit', edit_study_session_path(study_session) %></td>
    <td><%= link_to 'Destroy', study_session, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

